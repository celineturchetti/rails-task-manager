<h1>My Tasks</h1>
<ul>
  <% @tasks.each do |task| %>
  <% if task.completed == true %>
  <li><i class="fas fa-check-circle"></i> <%= link_to task.title, task_path(task) %></li>
    <% else %>
    <li><i class="fas fa-circle"></i> <%= link_to task.title, task_path(task) %></li>
    <% end %>
  <% end %>
</ul>

